<body style="background-color:white;">
    <div class="data-table-area mg-b-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <br />
                    <h3>   <a asp-controller="ECommerce" asp-action="AdminDashboard"> <button style="background-color:white;">Back</button></a></h3>
                    <table id="table" data-toggle="table" data-pagination="true" data-search="true" data-show-columns="true">
                        <thead>
                            <tr>
                                <th style="text-align:center">ChangeStatus</th>
                                <th style="text-align:center">ProductName</th>
                                <th style="text-align:center">Name</th>
                                <th style="text-align:center">MobileNumber</th>
                                <th style="text-align:center">Address</th>
                                <th style="text-align:center">City</th>
                                <th style="text-align:center">State</th>
                                <th style="text-align:center">PinCode</th>
                                <th style="text-align:center">Email</th>
                                <th style="text-align:center">AddressType</th>
                                <th style="text-align:center">Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null && Model.ListOrderModels != null)
                            {
                                @foreach (var item in Model.ListOrderModels)
                                {
                                    <tr>
                                        <td><a id="@item.OrderId" data-toggle="modal" data-target="#exampleModal" onclick="Openmodal(this.id);">Change</a></td>
                                        <td>Name</td>
                                        <td>@item.FullName</td>
                                        <td>@item.MobileNumber</td>
                                        <td>@item.Address</td>
                                        <td>@item.City</td>
                                        <td>@item.State</td>
                                        <td>@item.PinCode</td>
                                        <td>@item.Email</td>
                                        <td>@item.AddressType</td>
                                        <td>@item.Status</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    </div>

    <div class="modal fade MyModal" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-12">
                            <div class="form-select-list">
                                <select class="form-control custom-select-value" id="ddlProductStatus" name="account">
                                    <option value="0">Select</option>
                                    <option value="1">Order Processed</option>
                                    <option value="2">Order Shipped</option>
                                    <option value="3">Order in Route</option>
                                    <option value="4">Order Deliverd</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" id="btnSave" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="hdproductid" value="" />
