    <script>
         $("#btnSave").click(function () {
             ($('#hdproductid').val());
             ($('#ddlProductStatus').val());

            $.ajax({
                type: "GET",
                url: '@Url.Action("UpdateStatus")',
                data: { OrderId: $('#hdproductid').val(), Status: $('#ddlProductStatus').val() },
                dataType: "json",
                success: function () {
                    alert("Status has been added successfully..");
                    window.location.reload();
                },
                error: function () {
                    alert("Error while inserting data");
                }
            });

        });

        function Openmodal(OrderId) {
            $('#hdproductid').val(OrderId);
        }

    </script>
</body>



