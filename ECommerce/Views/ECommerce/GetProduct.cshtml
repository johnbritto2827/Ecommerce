    <div class="data-table-area mg-b-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="sparkline13-list shadow-reset" style="background-color:white">
                    </div>
                    <br />
                    <h1>   <a asp-controller="ECommerce" asp-action="ProductAdd"> <button type="submit" class="login-button login-button-lg">AddProduct</button></a></h1>
                    <table id="table" data-toggle="table" data-pagination="true" data-search="true" data-show-columns="true">
                        <thead>
                            <tr>
                                <th style="text-align:center">Brand</th>
                                <th style="text-align:center">Model</th>
                                <th style="text-align:center">Price</th>
                                <th style="text-align:center">Discount</th>
                                <th style="text-align:center">Specification</th>
                                <th style="text-align:center">QtyinStock</th>
                                <th style="text-align:center">Rating</th>
                                <th style="text-align:center">Colour</th>
                                <th style="text-align:center">Storage</th>
                                <th style="text-align:center">Image</th>
                                <th style="text-align:center">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null && Model.ListProductModels != null)
                            {
                                @foreach (var item in Model.ListProductModels)
                                {
                                    <tr>
                                        <td>@item.Brand</td>
                                        <td>@item.Model</td>
                                        <td>@item.Price</td>
                                        <td>@item.Discount</td>
                                        <td>@item.Specification</td>
                                        <td>@item.QtyinStock</td>
                                        <td>@item.Rating</td>
                                        <td>@item.Colour</td>
                                        <td>@item.Storage</td>
                                        <td>@item.Image</td>
                                        <td class="border px-4 py-2">
                                            <a class="" asp-route-ProductId="@item.ProductId" asp-controller="ECommerce" asp-action="EditProduct">
                                                <i class="fa fa-edit" style="color:highlight"></i>
                                            </a>
                                            <br /> <br />
                                            <a asp-route-ProductId="@item.ProductId" asp-controller="ECommerce" asp-action="DeleteProduct" onclick="return confirm('Are you sure to delete?')">
                                                <i class="fa fa-trash" style="color:red"></i>
                                            </a>
                                        </td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

